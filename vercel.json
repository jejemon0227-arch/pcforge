{
  "version": 2,
  "functions": {
    "functions/php.js": {
      "runtime": "nodejs18.x",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/test",
      "dest": "/functions/php.js"
    },
    {
      "src": "/time",
      "dest": "/functions/php.js"
    },
    {
      "src": "/info",
      "dest": "/functions/php.js"
    },
    {
      "src": "/json",
      "dest": "/functions/php.js"
    },
    {
      "src": "/(.*)",
      "dest": "/functions/php.js"
    }
  ],
  "builds": [
    {
      "src": "functions/php.js",
      "use": "@vercel/node"
    }
  ]
}
